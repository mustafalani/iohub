	-webkit-transition: .4s;
	transition: .4s;
}

.slider2:before {
	position: absolute;
	content: "";
	height: 8px;
	width: 8px;
	left: 2px;
	bottom: 2px;
	background-color: white;
	-webkit-transition: .4s;
	transition: .4s;
}

input:checked + .slider2 {
	background-color: #36af47;
}

input:focus + .slider2 {
