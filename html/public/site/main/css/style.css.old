}

input:checked + .slider2:before {
	-webkit-transform: translateX(8px);
	-ms-transform: translateX(8px);
	transform: translateX(8px);
}

/* Rounded sliders */
.slider2.round2 {
	border-radius: 34px;
}

.slider2.round2:before {
	border-radius: 50%;
}

